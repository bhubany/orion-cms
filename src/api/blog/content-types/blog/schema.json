{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "excerpt": {
      "type": "text",
      "minLength": 10,
      "maxLength": 300
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "tags": {
      "type": "component",
      "component": "page-properties.tags",
      "repeatable": true
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "blogs"
    }
  }
}
